  "description": "An OI4-compliant base service covering most of the \"mandatory\" functionality by the development guideline",
  "author": "Open Industry 4.0 Alliance - Community",
  "license": "BSD-3-Clause",
  "private": false,
  "main": "dist/app.js",
  "types": "dist/app.d.ts",
  "keywords": [
    "oi4",
    "service"
  ],
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:OI4/oi4-registry.git"
  },
  "dependencies": {
    "@oi4/oi4-oec-service-node": "^1.0.0-preview.3",
    "@types/ws": "^8.5.2",
    "axios": "^0.27.2",
    "ajv": "^6.12.6",
    "async-mqtt": "^2.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "glossy": "^0.1.7",
    "sequential-task-queue": "^1.2.1",
    "swagger-ui-express": "^4.3.0",
    "uuid": "^8.3.2",
    "winston": "^3.6.0",
    "winston-syslog": "^2.5.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "js-yaml": "^3.14.0"
  },
  "scripts": {
    "build": "yarn run clean && echo 'building service demo project' && tsc -b tsconfig.json",
    "clean": "rm -rf ./dist && rm -rf tsconfig.tsbuildinfo",
    "deploy": "echo \"Deploy not specified\" && exit 0",
    "lint": "eslint src/**/*.ts test/**/*.ts",
    "package": "echo \"Package not specified\" && exit 0",
    "test": "jest ",
    "docker:prepare": "docker buildx create --name oi4-builder --driver docker-container --use --platform linux/amd64,linux/arm64,linux/arm/v7,linux/arm/v6,linux/386",
    "docker:build:local": "docker build -t oi4/oi4-oec-service-demo:${npm_package_version} -t oi4/oi4-oec-service-demo:latest --build-arg PACKAGES_AUTH_TOKEN=${PACKAGES_AUTH_TOKEN} .",
    "docker:build:snapshot": "docker build -t oi4/oi4-oec-service-demo:${npm_package_version} -t oi4/oi4-oec-service-demo:latest --build-arg PACKAGES_AUTH_TOKEN=${PACKAGES_AUTH_TOKEN} --build-arg BUILD_ENV=snapshot .",
    "testLocal": "chmod ug+x ./scripts/test-local.sh && ./scripts/test-local.sh",
    "linkIt": "yarn link"
  },
  "gitHead": "334eba63fdecdf5c522c3158a96932b4e7d237b5"
}
